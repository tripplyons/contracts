{
    "extends": "default",
    "rules": {
        "use-forbidden-name": false,
        "indent": false,
        "statement-indent": false,
        "two-lines-top-level-separator": false,
        "not-rely-on-time": false,
        "no-complex-fallback": "warn",
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "double"
        ],
        "max-line-length": [
            "error",
            120
        ]
    }
}
